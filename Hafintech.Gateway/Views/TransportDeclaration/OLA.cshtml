                                            <option value="0">--Chọn--</option>
                                            <option v-for="item in lstInvCls" :value="item.invClsCd">{{item.invClsCd}} || {{item.invClsNm}}</option>
                                        </select>*@
                                </div>
                            </div>
                            <div class="control-group span12">
                                <label class="control-label">Phương tiện vận chuyển:</label>
                                <div class="controls">
                                    <input type="text" class="span12" v-model="data.lsCargoNo[index].transEquipCd" maxlength="300" /> ----
                                    <input type="text" class="span12" v-model="data.lsCargoNo[index].vesselNm" maxlength="300" />
                                </div>
