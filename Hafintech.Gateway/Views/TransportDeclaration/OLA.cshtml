                                    <input type="text" class="span7" autocomplete="off" />
                                    <button type="button" style="padding: 0 10px" onclick="hq.getSeachCustom()">...</button>
                                </div>
                            </div>
                            <div class="control-group span12">
                                <label class="control-label">Vị trí xếp hàng:</label>
                                <div class="controls">
                                    <input type="text" class="span3 CH" v-model="data.deptLocTransCd" />
