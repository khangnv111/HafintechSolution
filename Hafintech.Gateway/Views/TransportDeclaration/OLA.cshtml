                                    <button type="button" style="padding: 0 10px" onclick="hq.getSeachCustom()">...</button>
                                </div>
                            </div>
                            <div class="control-group span12">
                                <label class="control-label">Cảng /cửa khẩu / ga dỡ hàng:</label>
                                <div class="controls">
                                    <input type="text" class="span3 CH" v-model="data.arrLocBtransPd" />
                                    <input type="text" class="span7" v-model="data.arrLocBtransNm" autocomplete="off" />
                                    <button type="button" style="padding: 0 10px" onclick="hq.getSeachCustom()">...</button>
                                </div>
                            </div>
                            <div class="control-group span12">
                                <label class="control-label">Tuyến đường: </label>
                                <div class="controls">
                                    <input type="text" class="span11 TDN" v-model="data.route" maxlength="35" />
                                </div>
                            </div>
                            <div class="control-group span12">
                                <div class="form-vertical">
                                    <div class="control-group span6">
                                        <label class="control-label">Loại bảo lãnh:</label>
                                        <div class="controls">
                                            <input type="text" class="span11 TDN" v-model="data.typeOfSec" maxlength="35" />

                                            @*<select class="span10" v-model="data.typeOfSec">
                                                    <option value="0">--Chọn--</option>
                                                    <option v-for="item in lstInvCls" :value="item.invClsCd">{{item.invClsCd}} || {{item.invClsNm}}</option>
                                                </select>*@
                                        </div>
                                    </div>
                                    <div class="control-group span6">
                                        <label class="control-label">Mã ngân hàng bảo lãnh:</label>
                                        <div class="controls">
                                            <input type="text" id="txtsecSupplBankCd" name="txtsecSupplBankCd" class="span3 SBC" onclick="LibDec.GetIntroMess('IDA', 'SBC');" v-model="data.secSupplBankCd" />
                                            <input type="text" id="txtsecSupplBankCd_text" name="txtsecSupplBankCd_text" class="span7 SBC" onclick="LibDec.GetIntroMess('IDA', 'SBC');" />
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSeachBank(2)">...</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="control-group span12">
                                <div class="form-vertical">
                                    <div class="control-group span4">
                                        <label class="control-label">Năm phát hành bảo lãnh:</label>
                                        <div class="controls" style="position:relative;">
                                            <input type="text" class="span11" v-model="data.issuedYear" maxlength="4" />
                                        </div>
                                    </div>
                                    <div class="control-group span4">
                                        <label class="control-label">Kí hiệu chứng từ bảo lãnh:</label>
                                        <div class="controls" style="position:relative;">
                                            <input type="text" class="span11" v-model="data.secBankSign" maxlength="10" />
                                        </div>
                                    </div>
                                    <div class="control-group span4">
                                        <label class="control-label">Số chứng từ bảo lãnh:</label>
                                        <div class="controls" style="position:relative;">
                                            <input type="text" class="span11" v-model="data.secNo" maxlength="10" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="control-group span12">
                                <label class="control-label">Số tiền bảo lãnh: </label>
                                <div class="controls">
                                    <input type="text" class="span11 TDN" v-model="data.amountOfSec" maxlength="11" />VND
                                </div>
                            </div>
                            <div class="control-group span12">
                                <label class="control-label">Ghi chú:</label>
                                <div class="controls" style="position:relative">
                                    <textarea class="span11 NT2" v-model="data.remarks1"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="control-group span12">
                <label class="control-label">Nhập số vận đơn: </label>
                <div class="controls">
                    <input type="number" class="span11 TDN" v-model="cargoAmount" maxlength="11" />
                </div>
            </div>
            <div class="widget-box" v-for="(value,index) in data.lsCargoNo">
                <div class="widget-title">
                    <span class="icon"><i class="icon-th"></i></span>
                    <h5>Vận đơn ({{index+1}})</h5>
                </div>
                <div class="widget-content nopadding">
                    <div class="form-horizontal">
                        <div class="row-fluid">
