                txtphoneNoOfImp: "",
                txtconsignorNm: "",
                txtcountryCd_text: "",
                txtpieceUnitCd: "",
                txtpieceUnitCd_text: "",
                txtarrDate: "",
                txtloadLocNm: "",
                slinvClsCd: "",
                slinvPrcCdtCd: "",
                slinvPrcKindCd: "",
                txttotalInvPrc: "",
                hdfpieceUnitCd: "",
                txtweigUnitCdGrs: ""
            },
            submitHandler: function (form) {
                if (parseInt(decID) != 0) {
                    var checkTab = $("div[id='divTab2']").hasClass('noneTab');
                    if (checkTab == false) {
                        hq.confirmUpdateDeclaration_HV();
                    }
                }
                else {
                    hq.confirmInsertDeclaration_HV();
                }
            }
        });

        var today = new Date();
        var d = today.getDate();
        var m = today.getMonth() + 1;
        var y = today.getFullYear();
        if (d < 10) d = '0' + d;
        if (m < 10) m = '0' + m;
        today = m + '/' + d + '/' + y;

        $('.dDate').datetimepicker({
            showTodayButton: false,
            format: 'MM/DD/YYYY',
            minDate: today
        });

        $('.dDate2').datetimepicker({
            showTodayButton: false,
            format: 'MM/DD/YYYY',
        });

        //Nhóm loại hình
        hq.getGroupType();
        // Địa điểm trung chuyển
        hq.getTransportPlace();
        // Phương thức vận chuyển
        hq.getmeansOfTrsCd();
        // Mã loại Phí bảo hiểm
        hq.getInsuranceFeeCode();
        // Mã phân loại
        hq.getAdjustmentCode();
        // Mã bộ phận xử lý tờ khai
        hq.getDepartment();
        // Mã kết quả kiểm tra nội dung
        hq.getResult();
        // Phân loại cá nhân tổ chức
        hq.getOrganizationType();
        // Mã phân loại hàng hóa
        hq.getcargoClsCdCode();

        hq.GetAccountInfo();

        // Giấy phép nhập khẩu
        hq.getImportLicence();
        // Phân loại hình thức hóa đơn
        hq.getInvoiceType();
        //Phương thức thanh toán
        hq.getMethodPaid();
        // Mã phân loại giá hóa đơn
        hq.getinvPrcKindCd();
        // Điều kiện giá hóa đơn
        hq.getInvoiceValueCondition();
        //Danh sách loại tiền
        hq.getCurrency2();
        // Mã phân loại khai trị giá
        hq.getInvoiceValue();
        // Mã loại Phí vận chuyển
        hq.GetTransportFeeCodes();
        // Mã tên
        hq.getAdjName();

        // Mã lý do đề nghị bảo lãnh
        hq.getGuaranteeReasonCode();
        // Mã xác định thời hạn nộp thuế
        hq.getTaxExpiration();
        // Mã Phân loại đính kèm
        hq.getInvoiceAttach();

        

        var decID = "@(ViewBag.declarationID)";
        if (parseInt(decID) != 0) {
            $("div[id='divformC']").css("display", "none");
            $("div[id='divformU']").css("display", "");
            setTimeout(function () {
                hq.getDeclarationDetail_HV();
            }, 2000);
        }
        else {
            hq.GetAccountInfo();
        }

        var a = $("li[class='treeview act']");
        if (a.length > 1) {
            a[1].removeAttribute("class", "act");
        }
    })

