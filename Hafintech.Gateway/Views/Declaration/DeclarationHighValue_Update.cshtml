@{
    ViewBag.Title = "DeclarationHighValue_Update";
    Layout = "~/Views/Shared/_LayoutDeclaration.cshtml";
}

<form action="#" enctype="multipart/form-data" id="form-input" name="form-input">

    <div class="row-fluid">
        <div class="span12" id="divBtn">
            <h4><a href="@Url.Action("Declaration_List", "Declaration")">Danh sách Tờ khai</a> >> Tạo Tờ Khai:</h4>
            <a class="btn btn-warning" id="btnTab1" onclick="hq.OnClickTabDeclaration(1);">Thông tin chung</a>
            <a class="btn btn-success" id="btnTab2" onclick="hq.OnClickTabDeclaration(2);">Thông tin chung 2</a>
            <a class="btn btn-success">Danh sách hàng GTC</a>
            <a class="btn btn-success">Chỉ thị của hải quan</a>
            <a onclick="hq2.linkToCustomsMessage(2, 2)" class="btn btn-success">Kết quả xử lý tờ khai GTC</a>
            <a href="@Url.Action("ListVoucherCustom", "ElectronicVoucher", new { idDec = ViewBag.declarationID })&type=2" class="btn btn-success">Quản lý chứng từ điện tử</a>
            
        </div>
    </div>

    <div id="divTab1">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title">
                        <span class="icon"> <i class="icon-align-justify"></i> </span>
                        <h5>Thông Tin chung</h5>
                    </div>
                    <div class="widget-content nopadding">

                        <div class="form-horizontal">
                            <div class="span6">
                                <label class="control-label">Phân luồng: </label>
                                <div class="controls" id="phan-luong">
                                    Chưa phân luồng
                                </div>

                            </div>
                            <div class="span6">
                                <label class="control-label">Thông quan: </label>
                                <div class="controls" id="thong-quan">
                                    Chưa thông quan
                                </div>
                            </div>
                        </div>

                        <div class="form-horizontal">
                            <div class="row-fluid">
                                <div class="span12">
                                    <div class="control-group span12">
                                    </div>
                                </div>
                                <div class="span6" style="margin:0">
                                    <div class="control-group span12">
                                        <label class="control-label">Số Tờ khai: </label>
                                        <div class="controls">
                                            @if ((int)ViewBag.declarationID > 0)
                                            {
                                                <b style="line-height: 30px;" id="txtDeclaration">@ViewBag.declarationID</b>
                                                <input type="hidden" class="span5" id="hdfdclNo" name="hdfdclNo" value="@ViewBag.declarationID" />
                                                <input type="hidden" class="span5" id="dclNoHQ" name="dclNoHQ" />
                                            }
                                            else
                                            {
                                                <input type="text" class="span5 ICN" id="txtdclNo" name="txtdclNo" disabled onclick="LibDec.GetIntroMess('IDA', 'ICN')"/>
                                            }
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Số Tờ khai đầu tiên: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 FIC" id="txtfirstDclNo" name="txtfirstDclNo" onclick="LibDec.GetIntroMess('IDA', 'FIC')"/>
                                            <span>Số nhánh: </span>
                                            <input type="text" class="span2 BNO" id="txtbranchNoDivDcl" name="txtbranchNoDivDcl" onclick="LibDec.GetIntroMess('IDA', 'BNO')"/>/
                                            <input type="text" class="span2 DNO" id="txtnoDivisDivDcl" name="txtnoDivisDivDcl" onclick="LibDec.GetIntroMess('IDA', 'DNO')"/>
                                        </div>
                                    </div>

                                    <div class="control-group span12">
                                        <label class="control-label">Mã loại hình:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfdclKindCd" name="hdfdclKindCd" class="span6" />
                                            <input type="text" id="txtdclKindCd" name="txtdclKindCd" class="span3 ICB"  />
                                            <input type="text" id="txtdclKindCd_text" name="txtdclKindCd_text" class="span7 ICB" onclick="hq.getSeachMethod(); LibDec.GetIntroMess('IDA', 'ICB');" onchange="hq.getSeachMethod(); $('#hdfdclKindCd,#txtdclKindCd,#txtdclKindCd_text').val('');" autocomplete="off" />
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSeachMethod()">...</button>
                                        </div>
                                    </div>

                                    <div class="control-group span12">
                                        <label class="control-label">Mã phương thức vận chuyển:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <select id="slmeansOfTrsCd" name="slmeansOfTrsCd" class="span11 MTC" required onchange="hq.vaOnchangedata()" onclick="LibDec.GetIntroMess('IDA', 'MTC');">
                                                <option value="">--Chọn--</option>
                                            </select>

                                        </div>
                                    </div>

                                    <div class="control-group span12">
                                        <label class="control-label">Cơ quan hải quan:</label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfcstOffice" name="hdfcstOffice" />
                                            <input type="text" id="txtcstOffice" name="txtcstOffice" class="span3 CH" />
                                            <input type="text" id="txtcstOfficeNm" name="txtcstOfficeNm" class="span7" onclick="hq.getSeachCustom(); LibDec.GetIntroMess('IDA', 'CH');" onchange="hq.getSeachCustom(); $('#hdfcstOffice,#txtcstOfficeNm').val('');" autocomplete="off" />
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSeachCustom()">...</button>
                                        </div>
                                    </div>

                                    <div class="control-group span12">
                                        <label class="control-label">Thời hạn tái xuất:</label>
                                        <div class="controls" style="position:relative;">
                                            <input type="text" id="txttimeLimReExp" name="txttimeLimReExp" class="span11 dDate RED" onclick="LibDec.GetIntroMess('IDA', 'RED');"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="span6">
                                    <div class="control-group span12">
                                        <label class="control-label">Số TT: </label>
                                        <div class="controls">
                                            <input type="text" class="span11" id="txtOrdernumber" name="txtOrdernumber" disabled />
                                        </div>
                                    </div>

                                    <div class="control-group span12">
                                        <label class="control-label">Số tờ khai tạm nhập tái xuất tương ứng: </label>
                                        <div class="controls">
                                            <input type="number" min="0" class="span11 TDN" id="txttentativeDclNo" name="txttentativeDclNo" onclick="LibDec.GetIntroMess('IDA', 'TDN');" maxlength="12" />
                                        </div>
                                    </div>

                                    <div class="control-group span12">
                                        <label class="control-label">Mã phân loại hàng hóa:</label>
                                        <div class="controls">
                                            <select id="slcargoClsCd" name="slcargoClsCd" class="span11 CCC" onclick="LibDec.GetIntroMess('IDA', 'CCC')">
                                                <option value="">--Chọn--</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="control-group span12">
                                        <label class="control-label">Phân loại cá nhân tổ chức:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <select id="slclsOrg" name="slclsOrg" class="span11 SKB" required onclick="LibDec.GetIntroMess('IDA', 'SKB')">
                                                <option value="">--Chọn--</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã bộ phận xử lý tờ khai:</label>
                                        <div class="controls">
                                            <select id="slcstSubSection" name="slcstSubSection" class="span11 CHB" onclick="LibDec.GetIntroMess('IDA', 'CHB')">
                                                <option value="">--Chọn--</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Ngày khai báo (dự kiến):</label>
                                        <div class="controls" style="position:relative;">
                                            <input type="text" id="txtdclPlannedDate" name="txtdclPlannedDate" class="span11 dDate ICD" onclick="LibDec.GetIntroMess('IDA', 'ICD')"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title">
                        <span class="icon"> <i class="icon-align-justify"></i> </span>
                        <h5>Thông tin Xuất nhập khẩu:</h5>
                    </div>
                    <div class="widget-content nopadding">
                        <div class="form-horizontal">
                            <div class="row-fluid">
                                <div class="span12">
                                    <h5 style="margin-left:10px;">NGƯỜI NHẬP KHẨU</h5>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 IMC" id="txtimperCd" name="txtimperCd" maxlength="13" onclick="LibDec.GetIntroMess('IDA', 'IMC')"/>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Tên: </label>
                                        <div class="controls">
                                            <input type="text" class="span11 IMN" id="txtimperNm" name="txtimperNm" maxlength="100" onclick="LibDec.GetIntroMess('IDA', 'IMN')"/>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã bưu chính: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 IMY" id="txtpostcode" name="txtpostcode" maxlength="7" onclick="LibDec.GetIntroMess('IDA', 'IMY')"/>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Điện thoại: <span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="text" id="txtphoneNoOfImp" name="txtphoneNoOfImp" class="span6 IMT" maxlength="19" required onclick="LibDec.GetIntroMess('IDA', 'IMT')"/>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Địa chỉ: </label>
                                        <div class="controls">
                                            <input type="text" id="txtaddressOfImp" name="txtaddressOfImp" class="span11 IMA" maxlength="100" onclick="LibDec.GetIntroMess('IDA', 'IMA')"/>
                                        </div>
                                    </div>
                                    <h5 style="margin:10px 0 0 10px; float:left">NGƯỜI ỦY THÁC NHẬP KHẨU</h5>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 NMC" id="txtimpCtrCd" name="txtimpCtrCd" maxlength="13" onclick="LibDec.GetIntroMess('IDA', 'NMC')"/>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Tên: </label>
                                        <div class="controls">
                                            <input type="text" class="span11 NMN" id="txtimpCtrNm" name="txtimpCtrNm" maxlength="70" onclick="LibDec.GetIntroMess('IDA', 'NMN')" onkeyup="$('#txtimpCtrNm').val(hq.ConvertVietNamese($('#txtimpCtrNm').val()));" />
                                        </div>
                                    </div>
                                    <h5 style="margin:10px 0 0 10px; float:left">NGƯỜI XUẤT KHẨU</h5>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 EPC" id="txtconsignorCd" name="txtconsignorCd" maxlength="13" onclick="LibDec.GetIntroMess('IDA', 'EPC')"/>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Tên:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="text" class="span11 EPN" id="txtconsignorNm" name="txtconsignorNm" onclick="LibDec.GetIntroMess('IDA', 'EPN')" onkeyup="$('#txtconsignorNm').val(hq.ConvertVietNamese($('#txtconsignorNm').val()));" maxlength="70" required />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã bưu chính: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 EPY" id="txtpostcodeIdt" name="txtpostcodeIdt" maxlength="9" onclick="LibDec.GetIntroMess('IDA', 'EPY')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Địa chỉ: </label>
                                        <div class="controls">
                                            <p>
                                                <input type="text" class="span6 EPA" id="txtaddress1Street" name="txtaddress1Street" maxlength="35" onclick="LibDec.GetIntroMess('IDA', 'EPA')" onkeyup="$('#txtaddress1Street').val(hq.ConvertVietNamese($('#txtaddress1Street').val()));" />
                                                <input type="text" class="span6 EP2" id="txtaddress2Street" name="txtaddress2Street" maxlength="35" onclick="LibDec.GetIntroMess('IDA', 'EP2')" onkeyup="$('#txtaddress2Street').val(hq.ConvertVietNamese($('#txtaddress2Street').val()));" />
                                            </p>
                                            <p>
                                                <input type="text" class="span6 EP3" id="txtaddress3CityNm" name="txtaddress3CityNm" maxlength="35" onclick="LibDec.GetIntroMess('IDA', 'EP3')" onkeyup="$('#txtaddress3CityNm').val(hq.ConvertVietNamese($('#txtaddress3CityNm').val()));" />
                                                <input type="text" class="span6 EP4" id="txtcountryNmAddress" name="txtcountryNmAddress" maxlength="35" onclick="LibDec.GetIntroMess('IDA', 'EP4')" onkeyup="$('#txtcountryNmAddress').val(hq.ConvertVietNamese($('#txtcountryNmAddress').val()));" />
                                            </p>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã nước:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfcountryCd" name="hdfcountryCd" class="span3" />
                                            <input type="text" id="txtcountryCd" name="txtcountryCd" class="span3 EPO"  onclick="LibDec.GetIntroMess('IDA', 'EPO')"/>
                                            <input type="text" id="txtcountryNm" name="txtcountryNm EPO" class="span7" onclick="hq.getSeachCountry(); LibDec.GetIntroMess('IDA', 'EPO')" onchange="hq.getSeachCountry(); $('#hdfcountryCd,#txtcountryCd_text').val('');" required autocomplete="off" />
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSeachCountry()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Người ủy thác xuất khẩu: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 ENM" id="txtexportCsgNm" name="txtexportCsgNm" onclick="LibDec.GetIntroMess('IDA', 'ENM')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã người khai Hải quan: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 ICC" id="txtplannedDeclCd" name="txtplannedDeclCd" maxlength="5" onclick="LibDec.GetIntroMess('IDA', 'ICC')" onkeyup="$('#txtplannedDeclCd').val(hq.ConvertVietNamese($('#txtplannedDeclCd').val()));" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title">
                        <span class="icon"><i class="icon-th"></i></span>
                        <h5>Vận đơn</h5>
                    </div>
                    <div class="widget-content nopadding">
                        <div class="form-horizontal">
                            <div class="span12" style="padding-top:10px;">
                                @*<p style="padding:10px 0 0 15px"><input type="text" class="span4" id="txtNum1" name="txtNum1" style="width:50px" />&nbsp; <a>Khai báo số định danh theo đề án quản lý Giám sát Hải quan tự động tại Cảng biển.</a></p>
                                    <p style="padding-left:15px"><input type="text" class="span4" id="txtNum2" name="txtNum2" style="width:50px" />&nbsp; <a>Khai báo số định danh theo Quyết định 2061/QĐ-BTC(Cảng hàng không Quốc tế Nội Bài)</a></p>*@

                                <p class="text-center">
                                    <input type="text" class="span6 BL" id="txtcargoNo1" onclick="LibDec.GetIntroMess('IDA', 'BL');"
                                           name="txtcargoNo1" onkeyup="$('#txtcargoNo1').val(hq.ConvertVietNamese($('#txtcargoNo1').val()));" placeholder="Vận đơn 1" maxlength="35" />
                                    <input type="text" class="span6 BL" id="txtcargoNo2" onclick="LibDec.GetIntroMess('IDA', 'BL');"
                                           name="txtcargoNo2" onkeyup="$('#txtcargoNo2').val(hq.ConvertVietNamese($('#txtcargoNo2').val()));" placeholder="Vận đơn 2" maxlength="35" />
                                </p>

                                <p class="text-center">
                                    <input type="text" class="span6 BL" id="txtcargoNo3" onclick="LibDec.GetIntroMess('IDA', 'BL');"
                                           name="txtcargoNo3" onkeyup="$('#txtcargoNo3').val(hq.ConvertVietNamese($('#txtcargoNo3').val()));" placeholder="Vận đơn 3" maxlength="35" />
                                    <input type="text" class="span6 BL" id="txtcargoNo4" onclick="LibDec.GetIntroMess('IDA', 'BL');"
                                           name="txtcargoNo4" onkeyup="$('#txtcargoNo4').val(hq.ConvertVietNamese($('#txtcargoNo4').val()));" placeholder="Vận đơn 4" maxlength="35" />
                                </p>
                            </div>
                        </div>
                        <div class="form-horizontal">
                            <div class="row-fluid">
                                <div class="span6">
                                    <div class="control-group span12">
                                        <label class="control-label">Số lượng kiện:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfpieceUnitCd" name="hdfpieceUnitCd" class="span3" />
                                            <input type="number" min="1" id="txtcargoPiece" name="txtcargoPiece" class="span3 NO" required maxlength="8" onclick="LibDec.GetIntroMess('IDA', 'NO');"/>

                                            <span>
                                                <input type="text" id="txtpieceUnitCd_text" name="txtpieceUnitCd_text" class="span7 NOT"
                                                       required onclick="hq.getSeachSealUnit()" onchange="hq.getSeachSealUnit();$('#txtpieceUnitCd_text,#hdfpieceUnitCd').val('');" autocomplete="off" />
                                            </span>
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSeachSealUnit()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Phương tiện vận chuyển: </label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfTransport" name="hdfTransport" class="span3" />
                                            <input type="text" id="txtTransport" name="txtTransport VSC" class="span3" onclick="LibDec.GetIntroMess('IDA', 'VSC');"/>
                                            <input type="text" id="txtloadVesselAcNm" name="txtloadVesselAcNm" class="span7 VSN" onclick="LibDec.GetIntroMess('IDA', 'VSN');"
                                                   autocomplete="off" onkeyup="$('#txtloadVesselAcNm').val(hq.ConvertVietNamese($('#txtloadVesselAcNm').val()));" />
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSearchTransportation()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Địa điểm xếp hàng:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfloadLocCd" name="hdfloadLocCd" class="span6" />
                                            <input type="text" id="txtloadLocCd" name="txtloadLocCd" class="span3 PSC"  onclick="LibDec.GetIntroMess('IDA', 'PSC');"/>
                                            <input type="text" id="txtloadLocNm" name="txtloadLocNm" class="span7 PSN" 
                                                   onclick="hq.getSeachPlace()" onchange="hq.getSeachPlace(); $('#hdfPlace,#txtloadLocNm').val('');" autocomplete="off" />
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSeachPlace()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Địa điểm dỡ hàng: </label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfunloadPortCd" name="hdfunloadPortCd" class="span7" />
                                            <input type="text" id="txtunloadPortCd" name="txtunloadPortCd" class="span3 DST"  onclick="LibDec.GetIntroMess('IDA', 'DST');"/>
                                            <input type="text" id="txtunloadPortNm" name="txtunloadPortNm" class="span7 DSN" 
                                                   onclick="hq.getSeachUnPlace(); LibDec.GetIntroMess('IDA', 'DST')" onchange="hq.getSeachUnPlace(); $('#txtPlace2,#hdfPlace2').val('');" autocomplete="off" />
                                            <button type="button" style="padding: 0 10px" onclick="hq.getSeachUnPlace()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Số lượng Container:</label>
                                        <div class="controls">
                                            <input type="number" min="0" id="txtnoHandledCtn" name="txtnoHandledCtn" class="span6 COC" maxlength="3" onclick="LibDec.GetIntroMess('IDA', 'COC');"/>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã kết quả kiểm tra nội dung:</label>
                                        <div class="controls">
                                            <input type="text" id="txtresultInsCd" name="txtresultInsCd" class="span6"  />
                                            <select id="slresultInsCd" name="slresultInsCd" class="span6" onchange="hq.ResultOnChange();">
                                                <option value="">--Chọn--</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="span6">
                                    <div class="control-group span12">
