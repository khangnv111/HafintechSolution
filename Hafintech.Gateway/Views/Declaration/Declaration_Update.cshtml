@{
    ViewBag.Title = "Declaration_Update";
    Layout = "~/Views/Shared/_LayoutDeclaration.cshtml";
}

<form action="#" enctype="multipart/form-data" id="form-input-file" name="form-input-file">
    <div class="row-fluid">
        <div class="span12">
            <h4><a href="javascript:;">Danh sách Tờ khai</a> >> Tạo Tờ Khai:</h4>
            <a class="btn btn-warning" id="btnTab2"> Tờ khai </a>
            <a onclick="hq2.linkToCustomsMessage(1, 1);" class="btn btn-success">Kết quả xử lý tờ khai</a>
            @*<a class="btn btn-success">Danh sách hàng GTC</a>
            *@
            @*<a class="btn btn-success">Kết quả xử lý tờ khai</a>*@
            @*<a href="@Url.Action("Declaration_List", "Declaration")" class="btn btn-warning" style="float:right; margin: 0px 0px;"><i class="icon icon-book"></i> << Danh sách tờ khai </a>*@
            <div class="widget-box">
                <div class="widget-title">
                    <span class="icon"> <i class="icon-align-justify"></i> </span>
                    <h5>Thông Tin chung</h5>
                </div>
                <div class="widget-content nopadding">

                    <div class="form-horizontal">
                        <div class="span6">
                            <label class="control-label">Phân luồng: </label>
                            <div class="controls" id="phan-luong">
                                Chưa phân luồng
                            </div>
                        </div>
                        <div class="span6">
                            <label class="control-label">Thông quan: </label>
                            <div class="controls" id="thong-quan">
                                Chưa thông quan
                            </div>
                        </div>
                    </div>

                    <div class="form-horizontal">
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group span12">
                                    <label class="control-label">Mã Tờ khai: </label>
                                    <div class="controls">
                                        @if ((int)ViewBag.declarationID > 0)
                                        {
                                            <b style="line-height: 30px;" id="txtDeclaration">@ViewBag.declarationID</b>
                                            <input style="width:350px" type="hidden" class="span5" id="hdfdclNo" name="hdfdclNo" value="@ViewBag.declarationID" />
                                            <input type="hidden" class="span5" id="dclNoHQ" name="dclNoHQ" />
                                        }
                                        else
                                        {
                                            <input style="width:350px" type="text" class="span5 ICN" id="txtdclNo" name="txtdclNo" disabled onclick="LibDec.GetIntroMess('MIC', 'ICN');" />
                                        }
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Phân loại cá nhân tổ chức:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <select id="slclsOrg" name="slclsOrg" class="span11 SKB" required>
                                            <option value="">--Chọn--</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Cơ quan hải quan:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="hidden" id="hdfcstOffice" name="hdfcstOffice" />
                                        <input type="text" id="txtcstOffice" name="txtcstOffice" class="span3 CH"  onclick="LibDec.GetIntroMess('MIC', 'CH');" />
                                        <input type="text" id="txtcstOfficeNm" name="txtcstOfficeNm" class="span7 CH" onclick="hq.getSeachCustom(); LibDec.GetIntroMess('MIC', 'ST') " onchange="hq.getSeachCustom(); $('#hdfcstOffice,#txtcstOfficeNm').val('');" autocomplete="off" required />
                                        <button type="button" style="padding: 0 10px" onclick="hq.getSeachCustom()">...</button>
                                    </div>
                                </div>
                            </div>
                            <div class="span6">
                                <div class="control-group span12" style="margin-top:19%">
                                    <label class="control-label">Mã bộ phận xử lý tờ khai:</label>
                                    <div class="controls">
                                        <select id="slcstSubSection" name="slcstSubSection" class="span11 CHB" onclick="LibDec.GetIntroMess('MIC', 'CHB');">
                                            <option value="">--Chọn--</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid" style="display:none">
                            <div class="span12">
                                <div class="control-group span12">
                                    <label class="control-label">Văn Bản Liên Quan : </label>
                                    <div class="controls">
                                        <input name="file" type="file" id="file-input-docs" />
                                        <input type="hidden" id="dataFileDocsInput" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <!--<div class="widget-title">
                    <span class="icon"> <i class="icon-align-justify"></i> </span>
                    <h5>Thông tin Xuất nhập khẩu:</h5>
                </div>
                    -->
                <div class="widget-content nopadding">
                    <div class="form-horizontal">
                        <div class="row-fluid">
                            <div class="span12">
                                <h5 style="margin-left:10px;">NGƯỜI NHẬP KHẨU</h5>
                                <div class="control-group span12">
                                    <label class="control-label">Mã: </label>
                                    <div class="controls">
                                        <input type="text" class="span6 IMC" id="txtimperCd" name="txtimperCd" maxlength="13" onclick="LibDec.GetIntroMess('MIC', 'IMC')" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Tên: </label>
                                    <div class="controls">
                                        <input type="text" class="span11 IMN" id="txtimperNm" name="txtimperNm" maxlength="100" onclick="LibDec.GetIntroMess('MIC', 'IMN')" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Mã bưu chính: </label>
                                    <div class="controls">
                                        <input type="text" class="span6 IMY" id="txtpostcode" name="txtpostcode" maxlength="7" onclick="LibDec.GetIntroMess('MIC', 'IMY')" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Điện thoại: <span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="text" id="txtphoneNoOfImp" name="txtphoneNoOfImp" class="span6 IMT" maxlength="19" required onclick="LibDec.GetIntroMess('MIC', 'IMT')" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Địa chỉ: </label>
                                    <div class="controls">
                                        <input type="text" id="txtaddressOfImp" name="txtaddressOfImp" class="span11 IMA" maxlength="100" onclick="LibDec.GetIntroMess('MIC', 'IMA')" />
                                    </div>
                                </div>
                                <h5 style="margin:10px 0 0 10px; float:left">NGƯỜI XUẤT KHẨU</h5>
                                <div class="control-group span12">
                                    <label class="control-label">Mã: </label>
                                    <div class="controls">
                                        <input type="text" class="span6 EPC" id="txtconsignorCd" name="txtconsignorCd" maxlength="13" onclick="LibDec.GetIntroMess('MIC', 'EPC')" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Tên:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="text" class="span11 EPN" id="txtconsignorNm" name="txtconsignorNm" onclick="LibDec.GetIntroMess('MIC', 'EPN')" onkeyup="$('#txtconsignorNm').val(hq.ConvertVietNamese($('#txtconsignorNm').val()));" maxlength="70" required />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Mã bưu chính: </label>
                                    <div class="controls">
                                        <input type="text" class="span6 EPY" id="txtpostcodeIdt" name="txtpostcodeIdt" maxlength="9" onclick="LibDec.GetIntroMess('MIC', 'EPY')" />
                                    </div>
                                </div>

                                <div class="control-group span12">
                                    <label class="control-label">Mã nước: </label>
                                    <div class="controls">
                                        <input type="hidden" id="hdfcountryCd" name="hdfcountryCd" class="span3" value="" />
                                        <input type="text" id="txthdfcountryCd" name="txthdfcountryCd" class="span3 EPO"  onclick="LibDec.GetIntroMess('MIC', 'EPO');" />
                                        <button type="button" style="padding: 0 10px" onclick="hq.getSeachCountryExport();">...</button>
                                    </div>
                                </div>

                                <div class="control-group span12">
                                    <label class="control-label">Địa chỉ: </label>
                                    <div class="controls">
                                        <p>
                                            <input type="text" class="span6 EPA" id="txtaddress1Street" name="txtaddress1Street" maxlength="35" onclick="LibDec.GetIntroMess('MIC', 'EPA')" onkeyup="$('#txtaddress1Street').val(hq.ConvertVietNamese($('#txtaddress1Street').val()));" />
                                            <input type="text" class="span6 EP2" id="txtaddress2Street" name="txtaddress2Street" maxlength="35" onclick="LibDec.GetIntroMess('MIC', 'EP2')" onkeyup="$('#txtaddress2Street').val(hq.ConvertVietNamese($('#txtaddress2Street').val()));" />
                                        </p>
                                        <p>
                                            <input type="text" class="span6 EP3" id="txtaddress3CityNm" name="txtaddress3CityNm" maxlength="35" onclick="LibDec.GetIntroMess('MIC', 'EP3')" onkeyup="$('#txtaddress3CityNm').val(hq.ConvertVietNamese($('#txtaddress3CityNm').val()));" />
                                            <input type="text" class="span6 EP4" id="txtcountryNmAddress" name="txtcountryNmAddress" maxlength="35" onclick="LibDec.GetIntroMess('MIC', 'EP4')" onkeyup="$('#txtcountryNmAddress').val(hq.ConvertVietNamese($('#txtcountryNmAddress').val()));" />
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-content nopadding">
                    <div class="form-horizontal">
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group span12">
                                    <label class="control-label">Số House AWB:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input style="width:350px" type="text" class="span8 HAB" id="txthouseAwbNo" name="txthouseAwbNo" onclick="LibDec.GetIntroMess('MIC', 'HAB');"
                                               required maxlength="18" onkeyup="$('#txthouseAwbNo').val(hq.ConvertVietNamese($('#txthouseAwbNo').val()));" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Số lượng kiện:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="hidden" id="hdfpieceUnitCd" name="hdfpieceUnitCd" class="span3" />
                                        <input type="number" min="0" id="txtcargoPiece" name="txtcargoPiece" class="span10 NO" required maxlength="8" onclick="LibDec.GetIntroMess('MIC', 'NO');" />
                                        <span>
                                            Gói
                                        </span>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Mã địa điểm lưu kho hàng chờ thông quan dự kiến:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="hidden" id="hdfcstWrhCd" name="hdfcstWrhCd" class="span6" />
                                        <input type="text" id="txtcstWrhCd" name="txtcstWrhCd" class="span3 ST"  onclick="LibDec.GetIntroMess('MIC', 'ST');" />
                                        <input type="text" id="txtcstWrhCd_text" name="txtcstWrhCd_text" class="span7 ST" onclick="hq.getSeachUncstWrhCd(); LibDec.GetIntroMess('MIC', 'ST')" onchange="hq.getSeachUncstWrhCd(); $('#hdfcstWrhCd,#txtcstWrhCd_text').val('');" autocomplete="off" />
                                        <button type="button" style="padding: 0 10px" onclick="hq.getSeachUncstWrhCd()">...</button>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Tên máy bay chở hàng: </label>
                                    <div class="controls">
                                        <input style="width:350px" type="text" class="span8" id="txtflightNo"
                                               name="txtflightNo" onkeyup="$('#txtflightNo').val(hq.ConvertVietNamese($('#txtflightNo').val()));" maxlength="35" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Địa điểm dỡ hàng:<span class="error">(*)</span> </label>
                                    <div class="controls">
                                        <input type="hidden" id="hdfunloadPortCd" name="hdfunloadPortCd" class="span7" />
                                        <input type="text" id="txtunloadPortCd" name="txtunloadPortCd" class="span3 DST"  onclick="LibDec.GetIntroMess('MIC', 'DST');" />
                                        <input type="text" id="txtunloadPortNm" name="txtunloadPortNm" class="span7 DSN"
                                               onclick="hq.getSeachUnPlace(); LibDec.GetIntroMess('MIC', 'DST')" onchange="hq.getSeachUnPlace(); $('#txtPlace2,#hdfPlace2').val('');" autocomplete="off" />
                                        <button type="button" style="padding: 0 10px" onclick="hq.getSeachUnPlace()">...</button>
                                    </div>
                                </div>
                            </div>
                            <div class="span6">
                                <div class="control-group span12">
                                    <label class="control-label">Số Master AWB :</label>
                                    <div class="controls">
                                        <input style="width:350px" type="text" class="span8 MAB" id="txtmasterAwbNo" name="txtmasterAwbNo" maxlength="17" onclick="LibDec.GetIntroMess('MIC', 'MAB');"
                                               onkeyup="$('#txtmasterAwbNo').val(hq.ConvertVietNamese($('#txtmasterAwbNo').val()));" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Tổng trọng lượng hàng (Gross): <span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="hidden" id="hdfweigUnitCdGrs" name="hdfweigUnitCdGrs" class="span3" />
                                        <input type="number" min="0" id="txtcargoWeigGrs" name="txtcargoWeigGrs" class="span10 GW" maxlength="10" required onclick="LibDec.GetIntroMess('MIC', 'GW');"/>
                                        <span>KGM</span>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Ngày hàng đến:<span class="error">(*)</span></label>
                                    <div class="controls" style="position:relative;">
                                        <input type="text" id="txtarrDate" name="txtarrDate" class="span11 dDate ARR" required onclick="LibDec.GetIntroMess('MIC', 'ARR');" />
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Địa điểm xếp hàng:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="hidden" id="hdfloadLocCd" name="hdfloadLocCd" class="span6" />
                                        <input type="text" id="txtloadLocCd" name="txtloadLocCd" class="span3 PSC"  onclick="LibDec.GetIntroMess('MIC', 'PSC');" />
                                        <input type="text" id="txtloadLocNm" name="txtloadLocNm" class="span7 PSN"
                                               onclick="hq.getSeachPlace()" onchange="hq.getSeachPlace(); $('#hdfPlace,#txtloadLocNm').val('');" autocomplete="off" />
                                        <button type="button" style="padding: 0 10px" onclick="hq.getSeachPlace()">...</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-content nopadding">
                    <div class="form-horizontal">
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="control-group span12">
                                    <label class="control-label">Trị giá hóa đơn:</label>
                                    <div class="controls">
                                        <div class="span11">
                                            <select id="slinvPrcKindCd" name="slinvPrcKindCd" class="span3 IP1" style="width:167px" required onclick="LibDec.GetIntroMess('MIC', 'IP1');">
                                                <option value="">--Chọn giá trị--</option>
                                            </select>
                                            <span> - </span>
                                            <select id="slinvPrcCdtCd" name="slinvPrcCdtCd" class="span3 IP2" style="width:167px" required onclick="LibDec.GetIntroMess('MIC', 'IP2');">
                                                <option value="">--Chọn--</option>
                                            </select>
                                            <span> - </span>
                                            <select id="slinvCurCd" name="slinvCurCd" class="span3 IP3" style="width:167px" required onclick="LibDec.GetIntroMess('MIC', 'IP3');">
                                                <option value="">--Đồng tiền--</option>
                                            </select>
                                            <span> - </span>
                                            <input type="number" min="0" class="span12" id="txttotalInvPrc IP4" name="txttotalInvPrc" required maxlength="20" onclick="LibDec.GetIntroMess('MIC', 'IP4');" />
                                        </div>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Phí vận chuyển:</label>
                                    <div class="controls">
                                        <div class="span11">
                                            <select id="slfreightDemarCd" name="slfreightDemarCd" class="span3 FR1" style="width:167px" onclick="LibDec.GetIntroMess('MIC', 'FR1');">
                                                <option value="">--Chọn giá trị--</option>
                                            </select>
                                            <span> - </span>
                                            <select id="slfreightCurCd" name="slfreightCurCd" class="span3 FR2" style="width:167px" onclick="LibDec.GetIntroMess('MIC', 'FR2');">
                                                <option value="">--Đồng tiền--</option>
                                            </select>
                                            <span> - </span>
                                            <input type="number" min="0" class="span12 FR3" id="txtfreight" onclick="LibDec.GetIntroMess('MIC', 'FR3');"
                                                   name="txtfreight" style="max-width:530px; width:530px" maxlength="18" />
                                        </div>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Phí bảo hiểm:</label>
                                    <div class="controls">
                                        <div class="span11">
                                            <select id="slinsDemarCd" name="slinsDemarCd" class="span3 IN1" style="width:167px" onclick="LibDec.GetIntroMess('MIC', 'IN1');">
                                                <option value="">--Chọn giá trị--</option>
                                            </select>
                                            <span> - </span>
                                            <select id="slinsCurCd" name="slinsCurCd" class="span3 IN2" style="width:167px" onclick="LibDec.GetIntroMess('MIC', 'IN2');">
                                                <option value="">--Đồng tiền--</option>
                                            </select>
                                            <span> - </span>
                                            <input type="number" min="0" class="span12" id="txtinsAmt" name="txtinsAmt IN3" onclick="LibDec.GetIntroMess('MIC', 'IN3');" style="max-width:530px; width:530px" maxlength="16" />
                                        </div>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Mô tả hàng hóa: <span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input id="txtitemName" name="txtitemName" class="span11 CMN" required maxlength="200" onclick="LibDec.GetIntroMess('MIC', 'CMN');">
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Mã nước xuất xứ:<span class="error">(*)</span></label>
                                    <div class="controls">
                                        <input type="hidden" id="placeOriginCd" name="placeOriginCd" class="span3 OR" value="" />
                                        <input type="text" id="txtplaceOriginCd" name="txtplaceOriginCd" class="span3 OR"  onclick="LibDec.GetIntroMess('MIC', 'OR');" />
                                        <input type="text" id="txtcountryNm" name="txtcountryNm" class="span7 OR" onclick="hq.getSeachCountryOrigin(); LibDec.GetIntroMess('MIC', 'OR')" autocomplete="off" />
                                        <button type="button" style="padding: 0 10px" onclick="hq.getSeachCountryOrigin()">...</button>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Trị giá tính thuế :</label>
                                    <div class="controls">
                                        <input style="width:350px" type="number" min="0" maxlength="20" class="span8 DPR" id="txtcstValue" name="txtcstValue" onclick="LibDec.GetIntroMess('MIC', 'DPR');"/>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Phần ghi chú:</label>
                                    <div class="controls">
                                        <textarea id="txtnotes" name="txtnotes" class="span11 NT1" maxlength="100" onclick="LibDec.GetIntroMess('MIC', 'NT1');"></textarea>
                                    </div>
                                </div>
                                <div class="control-group span12">
                                    <label class="control-label">Số QL nội bộ của DN :</label>
                                    <div class="controls">
                                        <input style="width:350px" type="text" class="span8 REF" onclick="LibDec.GetIntroMess('MIC', 'REF');"
                                               id="txteiControlNo" name="txteiControlNo" maxlength="27" onkeyup="$('#txteiControlNo').val(hq.ConvertVietNamese($('#txteiControlNo').val()));" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span12">
            <div class="form-actions" style="text-align: center;">
                <button type="submit" class="btn btn-success" id="btnSaveDeclaration" style="display:none"></button>
                @if ((long)ViewBag.declarationID > 0)
                {
                    <a class="btn btn-success" onclick="hq.CheckSubmitDeclaration(1)">CẬP NHẬT TỜ KHAI</a>

                    <a class="btn btn-success" onclick="hq.confirmCloneDeclaration_HV('@ViewBag.declarationID')">COPY TỜ KHAI</a>
                }
                else
                {
                    <a class="btn btn-primary" onclick="">THÊM MỚI</a>
                    <a class="btn btn-warning" onclick="hq.CheckSubmitDeclaration(0)">LƯU NHÁP</a>
                    <a class="btn btn-success" onclick="hq.CheckSubmitDeclaration(1)">LƯU TỜ KHAI</a>
                    @*<a class="btn btn-danger" onclick="">XÓA TỜ KHAI</a>*@
                    <a class="btn btn-sm" onclick="">IN TỜ KHAI</a>
                    <a class="btn btn-warning" onclick="">XÁC THỰC TÀI KHOẢN</a>
                    <a class="btn btn-primary" onclick="">IN EXEL</a>
                }
            </div>
        </div>
    </div>
</form>

<script type="text/javascript">
    var decID = "@(ViewBag.declarationID)";
    $(document).ready(function () {

        $("#form-input-file").validate({
            rules: {
                slclsOrg: "required",
                txtcstOfficeNm: "required",
                txtcargoNo: "required",
                txtcargoPiece: "required",
                txtcstWrhCd_text: "required",
                txtloadVesselAcNm: "required",
                txtunloadPortNm: "required",
                txtcargoWeigGrs: "required",
                txtarrDate: "required",
                txtloadLocNm: "required",
                txtitemName: "required",
                txtcountryCd_text: "required",
                txtpieceUnitCd_text: "required",
                txtweigUnitCdGrs: "required",
                txtphoneNoOfImp: "required",
                slinvPrcKindCd: "required",
