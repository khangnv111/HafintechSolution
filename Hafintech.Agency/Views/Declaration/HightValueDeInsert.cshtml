@{
    ViewBag.Title = "HightValueDeInsert";
    Layout = "~/Views/Shared/_LayoutDeclaration.cshtml";
}

<form action="#" enctype="multipart/form-data" id="form-input" name="form-input">

    <div class="row-fluid">
        <div class="span12" id="divBtn">
            <h4><a href="#">Danh sách Tờ khai</a> >> Đăng ký mới Tờ Khai</h4>
            <a class="btn btn-warning" id="btnTab1" onclick="Decla.NextTabDecla(1)">Thông tin chung</a>
            <a class="btn btn-success" id="btnTab2" onclick="Decla.NextTabDecla(2)">Thông tin chung 2</a>
            @if (ViewBag.IdDec > 0)
            {
                <a href="@Url.Action("ListProduct", "Declaration")?IdDec=@ViewBag.IdDec&ishight=2&tab=1" class="btn btn-success">Danh sách hàng</a>
            }
            else
            {
                <a class="btn btn-success">Danh sách hàng</a>
            }

            <a class="btn btn-success">Chỉ thị của hải quan</a>
            <a onclick="hq2.linkToCustomsMessage(2)" class="btn btn-success">Kết quả xử lý tờ khai</a>
            <a onclick="hq2.linkToCustomsMessage(2)" class="btn btn-success">Quản lý tờ khai</a>
        </div>
    </div>

    <div id="divTab1">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title">
                        <span class="icon"> <i class="icon-align-justify"></i> </span>
                        <h5>Thông Tin chung</h5>
                    </div>
                    <div class="widget-content nopadding">
                        <div class="form-horizontal">
                            <div class="row-fluid">
                                <div class="span12">
                                    <div class="control-group span12">
                                        <div class="span3">
                                            <label class="control-label">Phần luồng: </label>
                                            <div class="controls" style="color: blue">
                                                {{stream}}
                                            </div>
                                        </div>

                                        <div class="span4">
                                            <label class="control-label">Thông quan: </label>
                                            <div class="controls" v-if="data.dateOfPermit">
                                                Thông quan
                                            </div>
                                            <div class="controls" v-else>
                                                Chưa thông quan
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="span6" style="margin:5px 0 0 0">
                                    <div class="control-group span12">
                                        <label class="control-label">Số Tờ khai: </label>
                                        <div class="controls">
                                            <input type="hidden" class="span5" id="dclId" name="dclId" value="@ViewBag.IdDec" />
                                            <input type="text" class="span5" id="dclNoHQ" name="dclNoHQ" value="" disabled v-model="data.dclNo" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Số Tờ khai đầu tiên: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 FIC" id="txtfirstDclNo" name="txtfirstDclNo" v-model="data.firstDclNo" v-on:click="getIntroMess('IDA','FIC')" />
                                            <span>Số nhánh: </span>
                                            <input type="text" class="span2 BNO" id="txtbranchNoDivDcl" name="txtbranchNoDivDcl" v-model="data.branchNoDivDcl" v-on:click="getIntroMess('IDA','BNO')" />/
                                            <input type="text" class="span2 DNO" id="txtnoDivisDivDcl" name="txtnoDivisDivDcl" v-model="data.noDivisDivDcl" v-on:click="getIntroMess('IDA','DNO')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã loại hình:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="text" id="txtdclKindCd" name="txtdclKindCd ICB" class="span3" v-model="data.dclKindCd" v-on:click="getIntroMess('IDA','ICB')" />
                                            <input type="text" id="txtdclKindCd_text" name="txtdclKindCd_text" class="span7 ICB" onclick="DePop.PopDecKind()" autocomplete="off" v-on:click="getIntroMess('IDA','ICB')" />
                                            <button type="button" style="padding: 0 10px" onclick="DePop.PopDecKind()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã phương thức vận chuyển:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <select id="slmeansOfTrsCd" name="slmeansOfTrsCd" class="span11 MTC" required v-model="data.meansOfTrsCd" v-on:click="getIntroMess('IDA','MTC')">
                                                <option value="">--Chọn--</option>
                                                <option v-for="item in lstMeansOfTrs" :value="item.meansOfTrsCd">{{item.meansOfTrsCd}} || {{item.meansOfTrsNm}}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="control-group span12" v-show="data.status==22">
                                        <label class="control-label">Mã số hàng hóa đại diện của tờ khai:</label>
                                        <div class="controls">
                                            <input type="text" id="txtrepTaxCd" name="txtrepTaxCd" class="span11 " v-model="data.repTaxCd" disabled />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Cơ quan hải quan:</label>
                                        <div class="controls">
                                            <input type="text" id="txtcstOffice" name="txtcstOffice" class="span3 CH" v-model="data.cstOffice" v-on:click="getIntroMess('IDA','CH')" />
                                            <input type="text" id="txtcstOfficeNm" name="txtcstOfficeNm" class="span7 CH" onclick="DePop.PopCustom()" autocomplete="off" v-on:click="getIntroMess('IDA','CH')" />
                                            <button type="button" style="padding: 0 10px" onclick="DePop.PopCustom()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12" v-show="data.status==22">
                                        <label class="control-label">Ngày đăng ký:</label>
                                        <div class="controls">
                                            <input type="text" id="regDate" name="regDate" class="span3" disabled="disabled" v-model="data.regDate" />
                                            <input type="text" id="regTime" name="regTime" class="span7" disabled v-model="data.regTime" />
                                        </div>
                                    </div>
                                    <div class="control-group span12" v-show="data.status==22">
                                        <label class="control-label">Ngày thay đổi đăng ký:</label>
                                        <div class="controls">
                                            <input type="text" id="regDateOfCor" name="regDateOfCor" class="span3" disabled="disabled" v-model="data.regDateOfCor" />
                                            <input type="text" id="regTimeOfCor" name="regTimeOfCor" class="span7" disabled v-model="data.regTimeOfCor" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Thời hạn tái xuất:</label>
                                        <div class="controls" style="position:relative;">
                                            <input type="date" id="txttimeLimReExp" name="txttimeLimReExp" class="span11 dDate RED" v-model="data.timeLimReExp" v-on:click="getIntroMess('IDA','RED')" />
                                        </div>
                                    </div>
                                </div>
                                <div class="span6">
                                    <div class="control-group span12">
                                        <label class="control-label">Số tờ khai tạm nhập tái xuất tương ứng: </label>
                                        <div class="controls">
                                            <input type="number" min="0" class="span11 TDN" id="txttentativeDclNo" name="txttentativeDclNo" maxlength="12" v-model="data.tentativeDclNo" v-on:click="getIntroMess('IDA','TDN')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12" v-show="data.status==22">
                                        <label class="control-label">Mã phân loại kiểm tra:</label>
                                        <div class="controls">
                                            <input type="text" id="txtinsClsCd" name="txtinsClsCd" class="span11" v-model="data.insClsCd" disabled />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã phân loại hàng hóa:</label>
                                        <div class="controls">
                                            <select id="slcargoClsCd" name="slcargoClsCd" class="span11 CCC" v-model="data.cargoClsCd" v-on:click="getIntroMess('IDA','CCC')">
                                                <option value="0">--Chọn--</option>
                                                <option v-for="item in lstCargoCls" :value="item.cargoClsCd">{{item.cargoClsCd}} || {{item.cargoClsNm}}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Phân loại cá nhân tổ chức:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <select id="slclsOrg" name="slclsOrg" class="span11 SKB" required v-model="data.clsOrg" v-on:click="getIntroMess('IDA','SKB')">
                                                <option value="">--Chọn--</option>
                                                <option v-for="item in lstOrganizationType" :value="item.clsOrgCd">{{item.clsOrgCd}} || {{item.clsOrgNm}}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã bộ phận xử lý tờ khai:</label>
                                        <div class="controls">
                                            <select id="slcstSubSection" name="slcstSubSection" class="span11 CHB" v-model="data.cstSubSection" v-on:click="getIntroMess('IDA','CHB')">
                                                <option value="0">--Chọn--</option>
                                                <option v-for="item in lstCstSubSection" :value="item.cstSubCd">{{item.cstSubCd}} || {{item.cstSubNm}}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Ngày khai báo (dự kiến):</label>
                                        <div class="controls" style="position:relative;">
                                            <input type="date" id="txtdclPlannedDate" name="txtdclPlannedDate" class="span11 dDate ICD" v-model="data.dclPlannedDate" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title">
                        <span class="icon"> <i class="icon-align-justify"></i> </span>
                        <h5>Thông tin Xuất nhập khẩu:</h5>
                    </div>
                    <div class="widget-content nopadding">
                        <div class="form-horizontal">
                            <div class="row-fluid">
                                <div class="span12">
                                    <h5 style="margin-left:10px;">NGƯỜI NHẬP KHẨU</h5>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 IMC" id="txtimperCd" name="txtimperCd" maxlength="13" v-model="data.imperCd" v-on:click="getIntroMess('IDA','IMC')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Tên: </label>
                                        <div class="controls">
                                            <input type="text" class="span11 IMN" id="txtimperNm" name="txtimperNm" maxlength="100" v-model="data.imperNm" v-on:click="getIntroMess('IDA','IMN')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã bưu chính: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 IMY" id="txtpostcode" name="txtpostcode" maxlength="7" v-model="data.postcode" v-on:click="getIntroMess('IDA','IMY')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Điện thoại: <span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="number" id="txtphoneNoOfImp" name="txtphoneNoOfImp" class="span6 IMT" maxlength="19" required v-model="data.phoneNoOfImp" v-on:click="getIntroMess('IDA','IMT')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Địa chỉ: </label>
                                        <div class="controls">
                                            <input type="text" id="txtaddressOfImp" name="txtaddressOfImp" class="span11 IMA" maxlength="100" v-model="data.addressOfImp" v-on:click="getIntroMess('IDA','IMA')" />
                                        </div>
                                    </div>
                                    <h5 style="margin:10px 0 0 10px; float:left">NGƯỜI ỦY THÁC NHẬP KHẨU</h5>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 NMC" id="txtimpCtrCd" name="txtimpCtrCd" maxlength="13" v-model="data.impCtrCd" v-on:click="getIntroMess('IDA','NMC')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Tên: </label>
                                        <div class="controls">
                                            <input type="text" class="span11 NMN" id="txtimpCtrNm" name="txtimpCtrNm" v-model="data.impCtrNm"
                                                   maxlength="70" onkeyup="$('#txtimpCtrNm').val($('#txtimpCtrNm').val(Decla.ConvertVietNamese($('#txtimpCtrNm').val()));" v-on:click="getIntroMess('IDA','NMN')" />
                                        </div>
                                    </div>
                                    <h5 style="margin:10px 0 0 10px; float:left">NGƯỜI XUẤT KHẨU</h5>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 EPC" id="txtconsignorCd" name="txtconsignorCd" maxlength="13" v-model="data.consignorCd" v-on:click="getIntroMess('IDA','EPC')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Tên:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="text" class="span11 EPN" id="txtconsignorNm" v-model="data.consignorNm"
                                                   name="txtconsignorNm" maxlength="70" onkeyup="$('#txtconsignorNm').val(Decla.ConvertVietNamese($('#txtconsignorNm').val()));" v-on:click="getIntroMess('IDA','EPN')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã bưu chính: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 EPY" id="txtpostcodeIdt" name="txtpostcodeIdt" maxlength="9" v-model="data.postcodeIdt" v-on:click="getIntroMess('IDA','EPY')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Địa chỉ: </label>
                                        <div class="controls">
                                            <p>
                                                <input type="text" class="span6 EPA" id="txtaddress1Street" name="txtaddress1Street" maxlength="35"
                                                       onkeyup="$('#txtaddress1Street').val(Decla.ConvertVietNamese($('#txtaddress1Street').val()));" v-model="data.address1Street" v-on:click="getIntroMess('IDA','EPA')" />
                                                <input type="text" class="span6 EP2" id="txtaddress2Street" name="txtaddress2Street" maxlength="35"
                                                       onkeyup="$('#txtaddress2Street').val(Decla.ConvertVietNamese($('#txtaddress2Street').val()));" v-model="data.address2Street" v-on:click="getIntroMess('IDA','EP2')" />
                                            </p>
                                            <p>
                                                <input type="text" class="span6 EP3" id="txtaddress3CityNm" name="txtaddress3CityNm" maxlength="35"
                                                       onkeyup="$('#txtaddress3CityNm').val(Decla.ConvertVietNamese($('#txtaddress3CityNm').val()));" v-model="data.address3CityNm" v-on:click="getIntroMess('IDA','EP3')" />
                                                <input type="text" class="span6 EP4" id="txtcountryNmAddress" name="txtcountryNmAddress" maxlength="35"
                                                       onkeyup="$('#txtcountryNmAddress').val(Decla.ConvertVietNamese($('#txtcountryNmAddress').val()));" v-model="data.countryNmAddress" v-on:click="getIntroMess('IDA','EP4')" />
                                            </p>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã nước:<span class="error">(*)</span></label>
                                        <div class="controls">
                                            <input type="text" id="txtcountryCd" name="txtcountryCd" class="span3 EPO" disabled="disabled" v-model="data.countryCd" v-on:click="getIntroMess('IDA','EPO')" />
                                            @*<input type="text" id="txtcountryNm" name="txtcountryNm" class="span7" onclick="hq.getSeachCountry()" onchange="hq.getSeachCountry(); $('#hdfcountryCd,#txtcountryCd_text').val('');" required autocomplete="off" />*@
                                            <button type="button" style="padding: 0 10px" onclick="DePop.PopCountry()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Người ủy thác xuất khẩu: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 ENM" id="txtexportCsgNm" name="txtexportCsgNm" v-model="data.exportCsgNm" v-on:click="getIntroMess('IDA','ENM')" />
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã người khai Hải quan: </label>
                                        <div class="controls">
                                            <input type="text" class="span6 ICC" id="txtplannedDeclCd" name="txtplannedDeclCd" v-model="data.plannedDeclCd"
                                                   maxlength="5" onkeyup="$('#txtplannedDeclCd').val(Decla.ConvertVietNamese($('#txtplannedDeclCd').val()));" v-on:click="getIntroMess('IDA','ICC')" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title">
                        <span class="icon"><i class="icon-th"></i></span>
                        <h5>Vận đơn</h5>
                    </div>
                    <div class="widget-content nopadding">
                        <div class="form-horizontal">
                            <div class="span12" style="padding-top:10px;">
                                <div class="control-group span12">
                                    <label class="control-label">Số vận đơn </label>
                                    <div class="controls" v-for="(value, index) in data.lsCargoNo">
                                        <input type="text" class="span4 BL" v-model="value.cargoNo" style="margin-right: 5px" v-on:click="getIntroMess('IDA','BL')" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-horizontal">
                            <div class="row-fluid">
                                <div class="span6">
                                    <div class="control-group span12">
                                        <div class="control-group span12">
                                            <label class="control-label">Số lượng kiện:<span class="error">(*)</span></label>
                                            <div class="controls">
                                                <input type="hidden" id="hdfpieceUnitCd" name="hdfpieceUnitCd" class="span5" v-model="data.pieceUnitCd" />
                                                <input type="text" id="txtcargoPiece" name="txtcargoPiece" class="span3 NO" required v-model="data.cargoPiece" v-on:click="getIntroMess('IDA','NO')" />
                                                <span>
                                                    <input type="text" id="txtpieceUnitCd_text" name="txtpieceUnitCd_text" class="span7 NOT" v-model="data.pieceUnitCd" v-on:click="getIntroMess('IDA','NOT')"
                                                           onclick="DePop.PopSealUnit()" autocomplete="off" />
                                                </span>
                                                <button type="button" style="padding: 0 10px" onclick="DePop.PopSealUnit()">...</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Mã địa điểm lưu kho hàng chờ thông quan dự kiến:</label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfcstWrhCd" name="hdfcstWrhCd" class="span6" v-model="data.cstWrhCd" />
                                            <input type="text" id="txtcstWrhCd" name="txtcstWrhCd" class="span3 ST" disabled="disabled" v-model="data.cstWrhCd" v-on:click="getIntroMess('IDA','ST')" />
                                            <input type="text" id="txtcstWrhCd_text" name="txtcstWrhCd_text" class="span7 ST" v-model="data.cstClrWrhNm" v-on:click="getIntroMess('IDA','ST')"
                                                   onclick="DePop.PopWareHouse()" autocomplete="off" />
                                            <button type="button" style="padding: 0 10px" onclick="DePop.PopWareHouse()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Phương tiện vận chuyển: </label>
                                        <div class="controls">
                                            <input type="text" id="txtTransport" name="txtTransport" class="span3 VSC" v-model="data.loadVesselCd" v-on:click="getIntroMess('IDA','VSC')"
                                                   onkeyup="$('#txtTransport').val($('#txtTransport').val().toUpperCase()); $('#hdfTransport').val($('#txtTransport').val().toUpperCase());" />
                                            <input type="text" id="txtloadVesselAcNm" name="txtloadVesselAcNm" class="span7 VSN" v-model="data.loadVesselAcNm" v-on:click="getIntroMess('IDA','VSN')"
                                                   onkeyup="$('#txtloadVesselAcNm').val($('#txtloadVesselAcNm').val().toUpperCase());" autocomplete="off" />
                                            <button type="button" style="padding: 0 10px" onclick="DePop.PopTransport();">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Địa điểm dỡ hàng: </label>
                                        <div class="controls">
                                            <input type="text" id="txtunloadPortCd" name="txtunloadPortCd" class="span3 DST" disabled="disabled" v-model="data.unloadPortCd" v-on:click="getIntroMess('IDA','DST')" />
                                            <input type="text" id="txtunloadPortNm" name="txtunloadPortNm" class="span7 DSN" onclick="DePop.PopUnplace()" autocomplete="off" v-on:click="getIntroMess('IDA','DSN')"
                                                   v-model="data.unloadPortCd" />
                                            <button type="button" style="padding: 0 10px" onclick="DePop.PopUnplace()">...</button>
                                        </div>
                                    </div>
                                    <div class="control-group span12">
                                        <label class="control-label">Số lượng Container:</label>
                                        <div class="controls">
                                            <input type="number" min="0" id="txtnoHandledCtn" name="txtnoHandledCtn" class="span6 COC" maxlength="3" v-model="data.noHandledCtn" v-on:click="getIntroMess('IDA','COC')" />
                                        </div>
                                    </div>
                                </div>
                                <div class="span6">
                                    <div class="control-group span12">
                                        <label class="control-label">Tổng trọng lượng hàng (Gross): </label>
                                        <div class="controls">
                                            <input type="hidden" id="hdfweigUnitCdGrs" name="hdfweigUnitCdGrs" class="span3" v-model="data.weigUnitCdGrs" />
