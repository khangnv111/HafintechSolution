        .mx-datepicker-popup {
            top: 19px !important;
        }
    </style>
    @if (Request.Url.ToString().Contains("10.1.20.2"))
    {
        <script src="http://10.1.20.2:8080/lib.signature.js"></script>
    }
    else
    {
        <script src="@System.Configuration.ConfigurationManager.AppSettings["UrlLibSign"]"></script>
    }
    
</head>
<body>

    @Html.Action("Header", "Home")

    @Html.Action("MenuHeader", "Home")

    <div id="content">
        <div class="container-fluid">
            <div class="menuLeft">
                @Html.Action("LeftMenu", "Declaration")
            </div>
            <div id="contentRight">
                @RenderBody()
            </div>
        </div>
    </div>

    <script type="text/javascript">
         

        $(window).scroll(function () {
            var h = $(document).scrollTop();
            if (h >= 123) {
                $("#content .menuLeft").css("position", "fixed");
                $("#content .menuLeft").css("top", "0");
            }
            else {
                $("#content .menuLeft").css("position", "");
            }
        });

        var h_web = $(document).height();
        var h1 = $("#header").height();
        var h2 = $("#sidebar").height();
        var min_h = h_web - h1 - h2;
        $("#content").css("min-height", min_h + "px"); 
         
    </script>
</body>
