                    parentId = $("#accIdLogin").val();
                }

                var urlSearch = Config.API_Login + "Agency/SearchPersonal";
                var dataSearch = {
                    personalId: 0,
                    identity: this.textSearchObj,
                    parentId: parentId,
                    type: 0
                }

                if (this.tabObj == 2) {
                    urlSearch = Config.API_Login + "Business/SearchBusiness";
