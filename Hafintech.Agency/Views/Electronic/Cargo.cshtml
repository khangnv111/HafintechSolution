                        parentId: parentId,
                        businessId: null,
                        taxIdNumber: this.textSearchObj,
                        status: 1,
                    }
                }

                var self = this;
                this.loadingObj = true;
                var token = $('#hdfToken').val();
                $.ajax({
                    type: 'POST',
                    url: urlSearch,
                    data: JSON.stringify(dataSearch),
