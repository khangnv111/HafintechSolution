@*v-for="(value,index) in data.lsCargoNo"*@

<div class="row-fluid">
    <div class="widget-box">
        <div class="widget-title">
            <span class="icon"><i class="icon-th"></i></span>
            <h5>Vận đơn ({{app.pageCargoNo}})</h5>
        </div>
        <div class="widget-content nopadding">
            <div class="form-horizontal">
                <div class="row-fluid">
                    <div class="control-group span12">
                        <label class="control-label">Số hàng hóa (Số B/L /AWB): <span class="error">(*)</span></label>
                        <div class="controls">
                            <input type="text" class="span3 EKN" v-model="CargoNo.corrType" v-show="data.status === 506" v-on:keyup="ChangeText()" style="text-transform:uppercase" v-on:click="getIntroMess('EDA','EKN')" />
                            <input type="text" class="span9 CG_" v-model="CargoNo.cargoNo" v-on:keyup="ChangeText()" style="text-transform:uppercase" v-on:click="getIntroMess('OLA','CG_')" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Ngày phát hành vận đơn: </label>
                        <div class="controls">
                            <input type="date" class="span11 DB_" v-model="CargoNo.issueDateOfBl" v-on:click="getIntroMess('OLA','DB_')" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Tên hàng:<span class="error">(*)</span> </label>
                        <div class="controls">
                            <input type="text" class="span11 CM_" v-model="CargoNo.goodsDes" maxlength="210" v-on:click="getIntroMess('OLA','CM_')" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Mã HS (4 số): </label>
                        <div class="controls">
                            <input type="text" class="span11 HS_" v-model="CargoNo.hSCd" maxlength="4" v-on:click="getIntroMess('OLA', 'HS_');" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Ký hiệu số hiệu: </label>
                        <div class="controls">
                            <textarea class="span11 MR_" v-model="CargoNo.marksAndNos" v-on:keyup="ChangeText()" style="text-transform:uppercase" v-on:click="getIntroMess('OLA', 'MR_');"></textarea>
                        </div>
                    </div>

                    <div class="control-group span12">
                        <div class="form-vertical">
                            <div class="control-group span6">
