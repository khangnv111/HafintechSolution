<div class="row-fluid">
    <div class="widget-box">
        <div class="widget-title">
            <span class="icon"> <i class="icon-align-justify"></i> </span>
            <h5>Thông Tin chung</h5>
        </div>
        <div class="widget-content nopadding">
            <div class="form-horizontal">
                <div class="span6">
                    <label class="control-label">Số tờ khai vận chuyển: </label>
                    <div class="controls" id="phan-luong">
                        <span style="color:blue;font-weight:bold;">{{data.btDclNo}}</span>
                    </div>
                </div>
            </div>
            <div class="form-horizontal">
                <div class="span12">
                    <label class="control-label span6">Cơ quan Hải quan tiếp nhận tờ khai: </label>
                    <div class="controls">
                        <input type="text" class="span6" v-model="data.subCstOffice" />
                    </div>
                </div>
            </div>
            <div class="form-horizontal">
                <div class="span12">
                    <label class="control-label">Cơ quan Hải quan địa điểm đích: </label>
                    <div class="controls">
                        <input type="text" class="span6" v-model="data.cstOffArrPlc" />
                    </div>
                </div>
            </div>

            <div class="form-horizontal">
                <div class="row-fluid">
                    <div class="control-group span6">
                        <label class="control-label">Cờ báo phân loại vận chuyển:</label>
                        <div class="controls">
                            @*<input type="text" class="span6" v-model="data.btTypeIn"/>*@
                            <select class="span10" v-model="data.btTypeIn">
                                <option value="A">A: Bonded transportation permitted cargo</option>
                                <option value="B">B: Bond-in to Customs Warehouse/Customs Manufacturing Warehouse permitted cargo</option>
                                <option value="C">C: Export declared cargo</option>
                            </select>
                        </div>
                    </div>
                    <div class="control-group span6">
                        <label class="control-label">Cờ báo nhập khẩu / xuất khẩu:</label>
                        <div class="controls">
                            <select class="span6 IEI" v-model="data.ieIndication" v-on:click="getIntroMess('OLA','IEI')">
                                <option value="I">Import</option>
                                <option value="E">Export</option>
                                <option value="C">Common</option>
                            </select>
                        </div>
                    </div>
                    <div class="control-group span6">
                        <label class="control-label">Cờ báo yêu cầu xác nhận/niêm phong:</label>
                        <div class="controls">
                            <input type="text" class="span6" v-model="data.cfirmReqSlIn" />
                        </div>
                    </div>
                    <div class="control-group span6">
                        <label class="control-label">Tiêu chuẩn kiểm tra đã được lựa chọn:</label>
                        <div class="controls">
                            <input type="text" class="span6" v-model="data.selectScrCrit" />
                        </div>
                    </div>

                    <div class="control-group span12">
                        <label class="control-label">Ngày lập tờ khai:</label>
                        <div class="controls">
                            <input type="date" class="span3 dDate" maxlength="8" v-model="data.dateOfDcl" />
                            <input type="text" class="span2 dDate" maxlength="8" v-model="data.dclrTime" />
                        </div>
                    </div>
                    <div class="control-group span6">
                        <label class="control-label">Ngày phê duyệt:</label>
                        <div class="controls">
                            <input type="date" class="span8 dDate" maxlength="8" v-model="data.approvalDate" />
                            <input type="text" class="span4 dDate" maxlength="8" v-model="data.approvalTime" />
                        </div>
                    </div>
                    <div class="control-group span6">
                        <label class="control-label">Mã Cán bộ Hải quan phê duyệt:</label>
                        <div class="controls">
                            <input type="text" class="span6" v-model="data.cstOffcerOfApr" />
                        </div>
                    </div>
                    <div class="control-group span4">
                        <label class="control-label">Ngày hủy bỏ:</label>
                        <div class="controls">
                            <input type="date" class="span8 dDate" maxlength="8" v-model="data.cancelDate" />
                            <input type="text" class="span4 dDate" maxlength="8" v-model="data.cancelTime" />
                        </div>
                    </div>
                    <div class="control-group span4">
                        <label class="control-label">Loại hủy bỏ:</label>
                        <div class="controls">
                            <select class="span8 IEI" v-model="data.cancelType">
                                <option value="CAN">CAN: Cancelled declaration</option>
                                <option value="COR">COR: Corrected declaration</option>
                                <option value="DIS">DIS: Disapproved declaration</option>
                                <option value="IND">IND: Indicated declaration</option>
                            </select>
                            @*<input type="text" class="span6" v-model="data.cancelType" />*@
                        </div>
                    </div>
                    <div class="control-group span4">
                        <label class="control-label">Mã cán bộ Hải quan hủy bỏ:</label>
                        <div class="controls">
                            <input type="text" class="span6" v-model="data.cstOffcerOfCan" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Người khai:</label>
                        <div class="controls">
                            <input type="text" class="span5" v-model="data.dclrCode" maxlength="13" />
                            <input type="text" class="span5" v-model="data.dclrName" maxlength="300" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Địa chỉ: </label>
                        <div class="controls">
                            <input type="text" class="span11" v-model="data.addrDclr" maxlength="300" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Nhà vận chuyển:<span class="error">(*)</span></label>
                        <div class="controls">
                            <input type="text" class="span5" v-model="data.transCd" maxlength="13" />
                            <input type="text" class="span5" v-model="data.transNm" maxlength="300" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Địa chỉ: </label>
                        <div class="controls">
                            <input type="text" class="span11" v-model="data.transAddress" maxlength="300" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Số hợp đồng vận chuyển / giấy tờ tương đương: </label>
                        <div class="controls">
                            <input type="text" class="span6" id="txtimpCtrCd" name="txtimpCtrCd" v-model="data.transCtrctNo" maxlength="13" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <div class="form-vertical">
                            <div class="control-group span6">
                                <label class="control-label">Ngày hợp đồng vận chuyển / giấy tờ:</label>
                                <div class="controls" style="position:relative;">
                                    <input type="date" class="span11 dDate" v-model="data.dateOfTrsCtrct" maxlength="8" />
                                </div>
                            </div>
                            <div class="control-group span6">
                                <label class="control-label">Ngày hết hạn hợp đồng vận chuyển / giấy tờ:</label>
                                <div class="controls" style="position:relative;">
                                    <input type="date" class="span11 dDate" v-model="data.expDateOfTrans" maxlength="8" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="control-group span12">
                        <div class="form-vertical">
                            <div class="control-group span4">
                                <label class="control-label">Mã phương tiện vận chuyển:<span class="error">(*)</span></label>

                                <div class="controls">
                                    @* <input type="text" class="span6" v-model="data.meansOfTrsCd" maxlength="13" />*@

                                    <select class="span10" v-model="data.meansOfTrsCd">
                                        <option value="6">6: Máy bay</option>
                                        <option value="11">11: Tàu thuỷ</option>
                                        <option value="16">16: Xà lan</option>
                                        <option value="25">25: Tàu hỏa</option>
                                        <option value="31">31: Ô tô</option>
                                        <option value="17">17: Khác</option>
                                    </select>
                                </div>
                            </div>
                            <div class="control-group span4">
                                <label class="control-label">Mã mục đích vận chuyển:<span class="error">(*)</span></label>
                                <div class="controls">
                                    @* <input type="text" class="span6" v-model="data.transPurposeCd" maxlength="13" />*@
                                    <select class="span10" v-model="data.transPurposeCd">
                                        <option value="ICD">ICD: hàng hóa nhập khẩu vận chuyển từ cửa khẩu nhập đến cảng đích ghi trên vận tải đơn (I)</option>
                                        <option value="IFS">IFS: Hàng hóa nhập khẩu được vận chuyển từ cửa khẩu nhập về địa điểm thu gom hàng lẻ (I)</option>
                                        <option value="ILS">ILS: hàng hóa nhập khẩu vận chuyển từ cửa khẩu nhập về kho hàng không kéo dài (I)</option>
                                        <option value="ITH">ITH:Hàng hóa nhập khẩu khác (I)</option>
                                        <option value="ECD">ECD: Hàng hóa xuất khẩu vận chuyển&nbsp; từ cảng xuất khẩu thành lập trong nội địa đến cửa khẩu xuất;</option>
                                        <option value="EFS">EFS: Hàng hóa xuất khẩu vận chuyển từ địa điểm thu gom&nbsp; hàng lẻ đến cửa khẩu xuất;</option>
                                        <option value="ELS">ELS: Hàng hóa xuất khẩu từ kho hàng không kéo dài đến cửa khẩu xuất;</option>
                                        <option value="EBD">EBD: Hàng hóa xuất khẩu vận chuyển từ kho ngoại quan đến cửa khẩu xuất;</option>
                                        <option value="ETH">ETH: Hàng hóa xuất khẩu khác</option>
                                        <option value="CTR">CTR: Hàng hóa quá cảnh</option>
                                        <option value="CTH">CTH: Hàng hóa khác</option>
                                    </select>
                                </div>
                            </div>
                            <div class="control-group span4">
                                <label class="control-label">Loại hình vận tải:<span class="error">(*)</span></label>
                                <div class="controls">
                                    @*<input type="text" class="span6" v-model="data.transType" maxlength="13" />*@

                                    <select class="span10" v-model="data.transType">
                                        <option value="EA">EA: Vận tải chung (Trường hợp vận chuyển từ tàu có kiểm tra tại cảng dỡ hàng)</option>
                                        <option value="QU">QU: Vận chuyển qua trạm kiểm dịch</option>
                                        <option value="KR">KR: Vận chuyển hàng hóa quá cảnh / quá cảnh</option>
                                        <option value="KS">KS: Vận chuyển đơn giản</option>
                                        <option value="PT">PT: Port đến Port giao thông vận tải</option>
                                        <option value="CT">CT: Giao thông với nhiều phương tiện giao thông</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Ngày dự kiến bắt đầu vận chuyển:<span class="error">(*)</span></label>
                        <div class="controls">
                            <input type="date" class="span3 dDate" v-model="data.esSdateOfTrans" maxlength="8" />
                            <input type="text" class="span2 dDate" v-model="data.esStimeOfTrans" maxlength="8" />
                        </div>
                    </div>
                    <div class="control-group span12">
                        <label class="control-label">Ngày dự kiến kết thúc vận chuyển:<span class="error">(*)</span></label>
                        <div class="controls">
                            <input type="date" class="span3 dDate" v-model="data.esFdateOfTrans" maxlength="8" />
                            <input type="text" class="span2 dDate" v-model="data.esFtimeOfTrans" maxlength="8" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

