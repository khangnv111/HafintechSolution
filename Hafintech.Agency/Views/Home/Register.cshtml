<div id="loginbox" style="width: 850px; transform: translate(-50%, -50%); top: 45%; left: 50%; position: fixed; z-index: 111">
    <div class="control-group normal_text">
        <b style="font-size:18px;">ĐĂNG KÝ THÔNG TIN DOANH NGHIỆP</b>
        <b onclick="utils.closeAll();" class="icClose">X</b>
    </div>

    <div class="control-group">
        <div class="controls">
            <div class="row-fluid" style="margin-bottom: 5px; text-align: left"> 
                <label style="text-align:left; width: 30%; font-weight:bold; display: inline-block">Mã doanh nghiệp (*)</label>
                <input type="text" class="form-control" id="CodeBusinessReg" style="width: 60%; height: 25px;" name="CodeBusiness" placeholder="Mã số thuế của doanh nghiệp cần đăng ký" />
            </div>
            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <label style="text-align:left; width: 30%; font-weight:bold; display: inline-block">Tên doanh nghiệp (*)</label>
                <input type="text" class="form-control" id="NameBusinessReg" name="NameBusiness" style="width: 60%;height: 25px;" placeholder="Tên doanh nghiệp" />
            </div>
            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Mật khẩu (*)</label>
                <input type="password" class="form-control" style="width: 30%; height: 25px" id="txtPasswordReg" placeholder="Mật khẩu phải đủ 8 ký tự bao gồm chữ hoa, chữ thường, số và ký tự đặc biệt" />
                @*<button onclick="accounts.showPassRegister()" id="btnshowPass"><img src="https://i.stack.imgur.com/waw4z.png" id="EYE"></button>*@
            </div>

            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Nhập lại mật khẩu (*)</label>
                <input type="password" class="form-control" id="txtRePassword" style="width: 30%; height: 25px" placeholder="Nhập lại mật khẩu" />
                @*<button onclick="accounts.showPassRegister()" id="btnshowPass"><img src="https://i.stack.imgur.com/waw4z.png" id="EYE"></button>*@
            </div>

            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Địa chỉ</label>
                <input type="text" class="form-control" id="AddressBusiness" style="width: 60%; height: 25px" placeholder="Địa chỉ doanh nghiệp" />
            </div>

            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <div style="width: 50%; float: left">
                    <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Người đại diện</label>
                    <input type="text" class="form-control" id="Representative" style="width: 60%; height: 25px" placeholder="Đại diện pháp luật của Doanh nghiệp" />
                </div>

                <div style="width: 50%; float: left">
                    <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Mã BC</label>
                    <input type="text" class="form-control" id="CodeBC" style="width: 60%; height: 25px" placeholder="Mã bưu chính" />
                </div>
            </div>

            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <div style="width: 50%; float: left">
                    <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Số điện thoại (*)</label>
                    <input type="text" class="form-control" id="txtPhone" style="width: 60%; height: 25px" placeholder="Nhập Số điện thoại" onkeypress="Vali.CheckOnlyNumber(this,event)" />
                </div>

                <div style="width: 50%; float: left">
                    <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Fax</label>
                    <input type="text" class="form-control" id="txtFac" style="width: 60%; height: 25px" placeholder="Fax" />
                </div>
            </div>
              
            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Email (*)</label>
                <input type="text" class="form-control" id="txtEmail" style="width: 60%; height: 25px" placeholder="Email" />
            </div>
            
            <div class="row-fluid" style="margin-bottom: 5px;text-align: left">
                <label style="text-align:left; font-weight:bold; width: 30%; display: inline-block">Đối tượng sử dụng (*)</label>
                <input type="radio" class="" id="typeUser1" name="typeUser" style="" value="2"/> Đại lý tờ khai
                <input type="radio" class="" id="typeUser2" name="typeUser" style="" value="1"/> Doanh nghiệp xuất nhập khẩu
            </div> 

            <div class="social-auth-links text-center">
                <p id="RegisterError" style="color:red;display:none;"></p>
            </div>
            <div class="main_input_box">
                <br />
                <a class="btn btn-primary" style="display:inline-block; max-width:150px; text-transform:uppercase" onclick="Reg.confirmRegister()">Đăng Ký</a>

                <a class="btn btn-warning" style="display:inline-block; max-width:150px; text-transform:uppercase" onclick="utils.closeAll()">Bỏ qua</a>
            </div>
        </div>
    </div>

   
</div>

<script type="text/javascript">
    $('#txtFullName,#txtEmail,#txtPhone,#txtIdentity,#txtPass').on('keypress', function (e) {
        if (e.which === 13) {
            Reg.confirmRegister();
        }
    });
</script>



