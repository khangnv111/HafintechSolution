                    switch (objVoucher.issuer) {
                        case "308": for (var i = 0; i < objVoucher.Licenses.length; i++) {
                            var fileNull = new File([""], objVoucher.Licenses[i].AttachedFile.fileName);
                            self.lstFiles.push(fileNull);
                        };
                            break;  //giấy phép
                        case "309": for (var i = 0; i < objVoucher.ContractDocuments.length; i++) {
                            var fileNull = new File([""], objVoucher.ContractDocuments[i].AttachedFile.fileName);
                            self.lstFiles.push(fileNull);
                        };
                            break; //hợp đồng
                        case "310": for (var i = 0; i < objVoucher.CommercialInvoices.length; i++) {
                            var fileNull = new File([""], objVoucher.CommercialInvoices[i].AttachedFile.fileName);
                            self.lstFiles.push(fileNull);
                        };
                            break;//hóa đơn
                        case "311": for (var i = 0; i < objVoucher.CertificateOfOrigins.length; i++) {
                            var fileNull = new File([""], objVoucher.CertificateOfOrigins[i].AttachedFile.fileName);
                            self.lstFiles.push(fileNull);
                        };
                            break;//CO
                        case "312": for (var i = 0; i < objVoucher.BillOfLadings.length; i++) {
                            var fileNull = new File([""], objVoucher.BillOfLadings[i].AttachedFile.fileName);
                            self.lstFiles.push(fileNull);
                        };
                            break;//vận đơn
                        case "313": var fileNull = new File([""], objVoucher.AttachedFile.fileName);
                            self.lstFiles.push(fileNull);
                            break;//container
                        case "314": for (var i = 0; i < objVoucher.AdditionalDocuments.length; i++) {
                            var fileNull = new File([""], objVoucher.AdditionalDocuments[i].AttachedFile.fileName);
                            self.lstFiles.push(fileNull);
                        };
                            break; //chứng từ khác
                    }



                    for (var i = 0; i < self.lstFiles.length; i++) {
                        var size = parseFloat(this.lstFiles[i].size / 1024).toFixed(2);
                        totalSize = +totalSize + +size;
                    }
                    if (totalSize > 3000) {
                        bootbox.alert("Tổng dung lượng file không được quá 3 MB");
                        return;
                    }

                    var formData = new FormData();
                    for (var i = 0; i < self.lstFiles.length; i++) {
                        if (self.lstFiles[i] != null) {
                            formData.append(self.lstFiles[i].name, self.lstFiles[i]);
                        }

                    }
                    var dataSend = objVoucher;

                    if (!dataSend.DeclarationDocument.natureOfTransaction) {
                        bootbox.alert("Bạn chưa nhập Mã Loại Hình");
                        utils.unLoading();
                        return;
                    }

                    //var dataTest = { "aa": "John", "bb": "30" }
                    formData.append("jsonData", JSON.stringify(dataSend));  //formData.append(name, value);

                    self.lstFiles = [];


                    $.ajax({
                        url: Config.API_Login + "Evoucher/update",
                        type: 'POST',
                        data: formData,
                        headers: {
                            "Authorization": "Bearer " + token
                        },
                        cache: false,
                        async: false,
                        contentType: false,
                        processData: false,
                        success: function (data) {
                            utils.unLoading();
                            if (data) {
                                if (data.ResponseCode > 0) {
                                    //bootbox.alert("Thành công!");
                                    //self.data = data.Data;
                                    if ($("#downSignkey").val() == 1 && utils.getCookie("signMethod") == 0) {
                                        bootbox.confirm("Bạn chưa có Chữ ký số hoặc Chữ số ký chưa cập nhật. <br> Mời bạn tải Chữ ký số", function (res) {
                                            if (res) {
                                                downTK();
                                            }
                                        });
                                        return;
                                    }

                                    this.statusTK = status;
                                    var data = {
                                        voucherId: itemVoucher.voucherId,
                                        "function": "8"
                                    }
                                    utils.Loading();

                                    var confirmOfDcl = "";
                                    if (status === 503 || status === 505) {
                                        confirmOfDcl = this.confirmOfDcl ? 1 : 0;
                                    }
                                    $.ajax({
                                        url: Config.API_Login + "Evoucher/GetDataToSign",//"Evoucher/GetEdiToSign",
                                        type: 'POST',
                                        data: JSON.stringify(data),
                                        headers: {
                                            "Authorization": "Bearer " + token
                                        },
                                        cache: false,
                                        async: false,
                                        contentType: "application/json; charset=utf-8",
                                        processData: false,
                                        success: function (data) {
                                            utils.unLoading();
                                            if (data) {
                                                if (data.ResponseCode >= 0) {
                                                    var ediStr = data.Data.replace('"', '');
                                                    self.messTK = ediStr;
                                                    self.voucherId = itemVoucher.voucherId;
                                                    self.dclNo = itemVoucher.dclId;
                                                    self.trinhky.sendWsJs(CMD.SIGN_TXT, FORMAT.BINARY, [ediStr]);
                                                }
                                                else {
                                                    utils.Message(JSON.stringify(data.Data));
                                                }
                                            }
                                            else {
                                                utils.Message("Hệ thống bận, vui lòng quay lại sau!");
                                            }
                                        },
                                        error: function (data) {
                                            utils.Message("Hệ thống bận, vui lòng quay lại sau!");
                                            utils.unLoading();
                                        }
                                    });
                                }
                                else {
                                    bootbox.alert(data.Description);
                                }
                            }

                            else {
                                utils.Message("Hệ thống bận, vui lòng quay lại sau!");
                            }
                            
                        },
                        error: function (data) {
                            utils.Message("Hệ thống bận, vui lòng quay lại sau!");
                            utils.unLoading();
                        }
                    });
                     
                }
                else {
                    bootbox.alert("không lấy được thông tin chứng từ, kiểm tra kết lại nối internet hoặc liên hệ kỹ thuật hỗ trợ!");
                    utils.unLoading();
                    return;
                }
                 
            },
            watchUSB: function (itemVoucher) {
                this.trinhky = new Trinhky(this.watchUSB_Callback);
                var self = this;
                var token = $('#hdfToken').val();
                //var data = this.data;
                if ($("#downSignkey").val() == 1 && utils.getCookie("signMethod") == 0) {
                    bootbox.confirm("Bạn chưa có Chữ ký số hoặc Chữ số ký chưa cập nhật. <br> Mời bạn tải Chữ ký số", function (res) {
