                        utils.unLoading();
                        if (data) {
                            if (data.ResponseCode >= 0) {
                                var ediStr = data.Data.replace('"', '');
                                self.messTK = ediStr;
                                self.voucherId = itemVoucher.voucherId;
                                self.dclNo = itemVoucher.dclId;
