                else {
                    bootbox.alert("không lấy được thông tin chứng từ, kiểm tra kết lại nối internet hoặc liên hệ kỹ thuật hỗ trợ!");
                    utils.unLoading();
                    return;
                }
                //b3 gọi hàm update voucher -> cập nhật số reference
                if (objVoucher != null) {
                    var totalSize = 0;
                    self.lstFiles = [];


                    if (objVoucher.Licenses == null) objVoucher.Licenses = [];
                    if (objVoucher.ContractDocuments == null) objVoucher.ContractDocuments = [];
